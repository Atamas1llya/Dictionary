{
  "manifest_version": 2,

  "name": "Pocket Dictionary",
  "description": "Pocket dictionary dedicated to help you remember words",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "popup.html",
   "default_title": "Dictionary"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    {"fileSystem": ["write"]}
  ]
}
